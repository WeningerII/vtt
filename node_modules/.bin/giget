#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/weningerii/vtt/node_modules/giget/dist/node_modules:/home/weningerii/vtt/node_modules/giget/node_modules:/home/weningerii/vtt/node_modules:/home/weningerii/node_modules:/home/node_modules:/node_modules:/home/weningerii/vtt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/weningerii/vtt/node_modules/giget/dist/node_modules:/home/weningerii/vtt/node_modules/giget/node_modules:/home/weningerii/vtt/node_modules:/home/weningerii/node_modules:/home/node_modules:/node_modules:/home/weningerii/vtt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../giget/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../giget/dist/cli.mjs" "$@"
fi
