{
  "name": "vtt",
  "private": true,
  "version": "0.0.0",
  "description": "Skeleton for a modern virtual tabletop.",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "eslint \"apps/**/src\" \"packages/**/src\" --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "dev": "turbo run dev --parallel",
    "dev:server": "tsx apps/server/src/index.ts",
    "dev:client": "cd apps/client && pnpm dev",
    "dev:bot": "tsx apps/bots/src/ws-smoke.ts",
    "stack:up": "docker compose up -d",
    "stack:down": "docker compose down",
    "db:gen": "pnpm dlx prisma generate --schema apps/server/prisma/schema.prisma",
    "db:migrate": "pnpm dlx prisma migrate dev --name init --schema apps/server/prisma/schema.prisma",
    "db:studio": "pnpm dlx prisma studio --schema apps/server/prisma/schema.prisma --port 5555"
  },
  "dependencies": {
