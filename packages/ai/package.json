{
  "name": "@vtt/ai",
  "version": "0.0.0",
  "private": true,
  "description": "Behavior trees and utility AI for non-player entities.",
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./src/providers/RealProviders": {
      "types": "./src/providers/RealProviders.ts",
      "import": "./src/providers/RealProviders.ts",
      "require": "./src/providers/RealProviders.ts"
    },
    "./src/circuit-breaker": {
      "types": "./src/circuit-breaker.ts",
      "import": "./src/circuit-breaker.ts", 
      "require": "./src/circuit-breaker.ts"
    },
    "./src/providers/CircuitBreakerProvider": {
      "types": "./src/providers/CircuitBreakerProvider.ts",
      "import": "./src/providers/CircuitBreakerProvider.ts",
      "require": "./src/providers/CircuitBreakerProvider.ts"
    }
  },
  "dependencies": {
    "@vtt/logging": "workspace:*",
    "@vtt/core-ecs": "workspace:*"
  },
  "peerDependencies": {
    "@vtt/performance": "*"
  },
  "peerDependenciesMeta": {
    "@vtt/performance": { "optional": true }
  },
  "scripts": {
    "build": "tsc -b",
    "lint": "eslint src --ext .ts"
  }
}